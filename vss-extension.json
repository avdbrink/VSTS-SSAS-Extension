{
    "manifestVersion": 1,
    "id": "VSTS-SSAS",
    "version": "1.0.7",
    "name": "SSAS Build & Deploy",
    "scopes": [ "vso.work", "vso.work_write" ],
    "description": "Extension with Continuous Integration support for SSAS",
    "baseUri": "https://github.com/avdbrink",
    "publisher": "avdbrink",
    "public": true,
    "icons": {
        "default": "images/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "SQL",
        "SSAS",
        "Analysis Services",
        "Tabular",
        "Denenv"
    ],
    "screenshots": [
        {
            "path": "docs/Screenshots/DeploySSAS-Task.png"
        }
    ],
    "links": {
        "home": {
            "uri": "https://github.com/avdbrink"
        },
        "getstarted": {
            "uri": "https://github.com/avdbrink"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/avdbrink"
    },
    "branding": {
        "color": "#f6f7fb",
        "theme": "light"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "files": [
        {
            "path": "DeploySSASTask"
        },
        {
            "path": "BuildSSASTask"
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }

    ],
    "contributions": [
        {
            "id": "DeploySSAS-task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "type": "ms.vss-distributed-task.task",
            "properties": {
                "name": "DeploySSASTask"
            }
        },
        {
            "id": "BuildSSAS-task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "type": "ms.vss-distributed-task.task",
            "properties": {
                "name": "BuildSSASTask"
            }
        }
    ]
}
